#include<iostream>
using namespace std;
class a
{

		public:
		a()
		{
			cout<<"constructor A is involved"<<endl;
		}		
	
		~a()
		{
			cout<<"destructor A is involved"<<endl;
		}		
};
class b: public a
{
		public:
		b()
		{
			cout<<"constructor B is involved"<<endl;
		}		
	
		~b()
		{
			cout<<"destructor B is involved"<<endl;
		}	
};
int main()
{
	b B;
}
